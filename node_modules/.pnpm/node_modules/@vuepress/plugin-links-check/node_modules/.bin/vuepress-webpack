#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules/vuepress/bin/node_modules:/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules/vuepress/node_modules:/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules:/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules/vuepress/bin/node_modules:/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules/vuepress/node_modules:/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules:/mnt/d/project/frontend/vuePressWiki/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
else
  exec node  "$basedir/../../../../../../vuepress@2.0.0-rc.8_@vuepress+bundler-vite@2.0.0-rc.7_vue@3.4.21/node_modules/vuepress/bin/vuepress-webpack.js" "$@"
fi
